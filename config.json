{
    "paths":{
        "raw_data":  "./raw_data/",
        "auxiliary_data": "./auxiliary_data/",
        "processed_data" : "./processed_data/",
        "checkpoints": "./checkpoints/",
        "logs": "./logs/"
    },
    "data_attr":{
        "data_sample_frequency": 25,
        "start_pos": 58.5,
        "end_pos": 63.5
    },
    "preprocess":{
        "spatial_stride": 320, 
        "spatial_window": 320,
        "temporal_stride": 160, 
        "temporal_window": 160,
        "agent_points_per_side_length":48,
        "agent_points_per_side_width": 16
    },
    "occ_flow_map":{
        "grid_size_x": 256,
        "grid_size_y": 256
    },
    "dataset":{
        "train_ratio": 0.8,
        "val_ratio": 0.1,
        "test_ratio": 0.1
    },
    "dataloader":{
        "num_workers": 1,
        "batch_size": 2,
        "shuffle": false
    },
    "task":{
        "his_len": 40,
        "pred_len": 120,
        "num_waypoints": 12
    },
    "training": {
        "checkpoint_interval": 1,
        "epochs": 20,
        "optimizer" :{
            "lr": 1e-4,
            "weight_decay": 0.0001
        },
        "scheduler":{
            "step_size": 3,
            "gamma": 0.5
        },
        "loss":{
            "loss_type": "L2",
            "loss_weight": 1.0
        }

    },
    "model" :{
        "input_size": [256, 256],
        "window_size": 8,
        "embed_dim": 96,
        "depths": [2, 2, 2],
        "num_heads": [3, 6, 12],
        "his_len": 40,
        "large_input": false
    }


}